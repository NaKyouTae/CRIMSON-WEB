{
  "name": "crimson-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CRIMSON-WEB - 거기어때 웹 애플리케이션",
  "author": "CRIMSON-WEB Team",
  "keywords": [
    "react",
    "vite",
    "crimson",
    "web"
  ],
  "scripts": {
    "dev": "npm run proto:generate && vite",
    "build": "npm run proto:generate && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "start": "vite",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./generated --ts_proto_opt=esModuleInterop=true,forceLong=number,useOptionals=messages --proto_path=./public/proto ./public/proto/**/*.proto",
    "prebuild": "npm run proto:generate"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "protobufjs": "^7.5.4",
    "ts-proto": "^2.7.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
